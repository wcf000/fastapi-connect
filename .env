# Grafana Configuration
# Copy this file to .env and customize for your environment

# --- Basic Connection Settings ---
GRAFANA_PORT=3000
GRAFANA_URL=http://localhost:3000
GRAFANA_API_KEY=your_api_key_here

# --- SSL Configuration ---
GRAFANA_SSL_VERIFY=false

# --- Retry Configuration ---
GRAFANA_RETRY_MAX_ATTEMPTS=3
GRAFANA_RETRY_WAIT_MULTIPLIER=1000

# --- Dashboard Settings ---
GRAFANA_HEALTH_TIMEOUT=30
GRAFANA_DASHBOARD_PATH=/etc/grafana/provisioning/dashboards
GRAFANA_UPDATE_INTERVAL=10

# --- Default Labels ---
GRAFANA_DEFAULT_SERVICE=lead_ignite
GRAFANA_ENVIRONMENT=development

# --- Prometheus Configuration ---
GRAFANA_MULTIPROC_DIR=/tmp/prometheus

# --- Circuit Breaker Configuration ---
GRAFANA_CIRCUIT_FAILURE_THRESHOLD=5
GRAFANA_CIRCUIT_RECOVERY_TIMEOUT=300

# --- Connection Pool Settings ---
GRAFANA_POOL_CONNECTIONS=20
GRAFANA_POOL_MAXSIZE=100
GRAFANA_MAX_RETRIES=3
GRAFANA_CONNECT_TIMEOUT=3.05
GRAFANA_READ_TIMEOUT=30.0

# --- Basic Configuration ---
PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=9090
PROMETHEUS_SERVICE_URL=http://localhost:9090

# --- Performance Settings ---
PROMETHEUS_HEALTH_TIMEOUT=30
PROMETHEUS_SCRAPE_INTERVAL=15

# --- Labeling ---
PROMETHEUS_METRICS_PREFIX=lead_ignite_
PROMETHEUS_DEFAULT_LABELS='{"service":"lead_ignite","environment":"production"}'

# OpenTelemetry Configuration
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_EXPORTER_OTLP_METRICS_ENDPOINT=http://localhost:4317
OTEL_EXPORTER_OTLP_LOGS_ENDPOINT=http://localhost:4317
OTEL_EXPORTER_OTLP_INSECURE=true
SERVICE_NAME=fastapi-connect
SERVICE_VERSION=1.0.0
ENVIRONMENT=local
ENABLE_PROMETHEUS=true

# --- Connection Settings ---
PULSAR_ADVERTISED_ADDRESS=localhost
PULSAR_BROKER_PORT=6650
PULSAR_SERVICE_URL=pulsar://localhost:6650

# --- Authentication ---
PULSAR_AUTH_TOKEN=
PULSAR_JWT_TOKEN=

# --- TLS/SSL Configuration ---
PULSAR_TLS_ENABLED=true
PULSAR_TLS_CERT_PATH=/path/to/cert
PULSAR_TLS_KEY_PATH=/path/to/key
PULSAR_TLS_CA_PATH=/path/to/ca

# --- Producer Settings ---
PULSAR_PRODUCER_SEND_TIMEOUT_MS=30000
PULSAR_PRODUCER_BLOCK_IF_QUEUE_FULL=true
PULSAR_PRODUCER_MAX_PENDING_MESSAGES=1000
PULSAR_PRODUCER_BATCHING_ENABLED=true
PULSAR_PRODUCER_BATCHING_MAX_MESSAGES=1000
PULSAR_PRODUCER_BATCHING_MAX_DELAY_MS=10

# --- Consumer Settings ---
PULSAR_CONSUMER_SUBSCRIPTION_TYPE=Shared
PULSAR_CONSUMER_ACK_TIMEOUT_MS=30000
PULSAR_CONSUMER_NACK_REDELIVERY_DELAY_MS=60000
PULSAR_CONSUMER_RECEIVER_QUEUE_SIZE=1000
PULSAR_CONSUMER_DLQ_MAX_REDELIVER_COUNT=3
PULSAR_CONSUMER_DLQ_TOPIC=persistent://tenant/namespace/dlq-topic

# --- Retry Settings ---
PULSAR_RETRY_MAX_RETRIES=5
PULSAR_RETRY_INITIAL_BACKOFF_MS=100
PULSAR_RETRY_MAX_BACKOFF_MS=10000

# --- Monitoring ---
